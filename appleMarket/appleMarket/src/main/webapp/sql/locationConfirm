# 동네인증[locationConfirm]

create table locationConfirm(
    member_id varchar2(200) not null, 
    location_addr varchar2(200) not null, 
    location_dong varchar2(10) not null,
    location_latitude varchar2(50) not null, 
    location_longitude varchar2(50) not null, 
    location_logtime date default sysdate
);