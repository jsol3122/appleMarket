<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="adminSQL">
 	
 	<!-- 회원 리스트 출력  -->
 	<select id="getAdminMemberList" resultType="member">
 		select member_seq, member_id, member_email, member_tel1, member_tel2, member_tel3, member_siteCheck, member_reputation from appleMarket
 	</select>
 	
 	<!-- 회원 삭제  -->
 	<delete id="adminMemberDelete" parameterType="member">
 		delete from appleMarket where member_id=#{member_id}
 	</delete>
 </mapper>
 	