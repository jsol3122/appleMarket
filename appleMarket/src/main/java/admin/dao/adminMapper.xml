<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="adminSQL">
 	
 	<!-- 회원 리스트 출력  -->
 	<select id="adminMemberList" resultType="member">
 		select * from appleMarket order by member_entryLogtime
 	</select>
 	
 	<!-- 회원 삭제  -->
 	<delete id="adminMemberDelete" parameterType="member">
 		delete from appleMarket where member_id=#{member_id}
 	</delete>
 </mapper>
 	